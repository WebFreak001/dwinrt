{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dub",
			"label": "dub build generator",
			"run": false,
			"cwd": ".",
			"compiler": "$current",
			"archType": "$current",
			"buildType": "$current",
			"configuration": "$current",
			"problemMatcher": [
				"$dmd"
			],
			"dub_args": [
				"--parallel"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"clear": true
			}
		}
	]
}
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dub",
			"label": "dub build generator",
			"detail": "dub build",
			"run": false,
			"cwd": ".",
			"compiler": "$current",
			"archType": "$current",
			"buildType": "$current",
			"configuration": "application",
			"problemMatcher": [
				"$dmd"
			],
			"dub_args": [
				"--parallel"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"clear": true
			}
		},
		{
			"type": "dub",
			"label": "dub test generator",
			"detail": "dub test",
			"run": true,
			"cwd": ".",
			"compiler": "$current",
			"archType": "$current",
			"buildType": "unittest",
			"configuration": "unittest",
			"problemMatcher": [
				"$dmd"
			],
			"dub_args": [
				"--parallel"
			],
			"group": {
				"kind": "test"
			},
			"presentation": {
				"clear": true
			}
		}
	]
}